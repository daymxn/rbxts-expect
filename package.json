{
  "name": "@rbxts/expect",
  "version": "2.0.2",
  "description": "Test-agnostic assertion library for ROBLOX.",
  "main": "out/init.luau",
  "scripts": {
    "build": "rbxtsc -p ./tsconfig.release.json",
    "build:dev": "rbxtsc -p --type game --rojo test.project.json",
    "watch": "pnpm build:dev -w",
    "test": "pnpm bs test",
    "help": "pnpm bs help"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "target": "esnext",
    "dts": {
      "only": true
    }
  },
  "keywords": [
    "rbxts",
    "expect",
    "test",
    "assertion",
    "typescript",
    "roblox"
  ],
  "author": "Daymon Littrell-Reyes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daymxn/rbxts-expect.git"
  },
  "bugs": {
    "url": "https://github.com/daymxn/rbxts-expect/issues"
  },
  "license": "Apache-2.0",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/tests/**",
    "!**/*.tsbuildinfo",
    "!**/*.spec.lua",
    "!**/*.spec.luau",
    "!**/*.spec.ts",
    "!**/*.spec.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@microsoft/api-documenter": "^7.26.0",
    "@microsoft/api-extractor": "^7.48.0",
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/testez": "git+https://github.com/daymxn/testez.git#patch-1",
    "@rbxts/types": "^1.0.813",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "bs-cli": "github:daymxn/bs-cli#main",
    "eslint": "9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-headers": "^1.2.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-roblox-ts": "github:daymxn/eslint-plugin-roblox-ts#feat/upgrade-to-eslint-9",
    "eslint-plugin-unicorn": "^56.0.1",
    "prettier": "^3.4.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "roblox-ts": "^3.0.0-next",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "=5.5.3",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "@eslint/js": "^9.15.0",
    "@rbxts/deep-equal": "^0.8.1",
    "@rbxts/object-utils": "^1.0.4",
    "@rbxts/reverse-array": "^1.0.3",
    "@rbxts/rust-classes": "0.13.0",
    "@rbxts/services": "^1.5.5",
    "@rbxts/string-utils": "^1.0.3",
    "@rbxts/t": "^3.1.1",
    "jiti": "^2.4.0"
  },
  "peerDependencies": {
    "roblox-ts": "^3.0.0"
  }
}